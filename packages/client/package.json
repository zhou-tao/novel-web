    "echarts": "^5.4.1",
    "element-plus": "^2.2.18",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.3.0",
    "qs": "^6.11.0",
    "viewerjs": "^1.11.3",
    "vue": "3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify-json/ep": "^1.1.8",
    "@iconify-json/fluent-emoji-flat": "^1.1.8",
